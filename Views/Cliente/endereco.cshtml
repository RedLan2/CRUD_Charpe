@model CRUD.Models.Endereco

<div>
    <div>
         <form asp-action="endereco">
            <div>
                  <div class="form-group">   
                            <input asp-for="Rua" class="form-control" placeholder="Rua">
                    </div>
                    <div class="form-group">   
                            <input asp-for="CEP" class="form-control" placeholder="CEP">
                    </div>
                     <div class="form-group">   
                            <input asp-for="Cidade" class="form-control" placeholder="Cidade">
                    </div>
                     <div class="form-group">   
                            <input asp-for="Estado" class="form-control" placeholder="Estado">
                    </div>
                     <div class="form-group">   
                            <input asp-for="Numero" class="form-control" placeholder="Numero">
                    </div>
                    <div>
                            <input type="submit" value="Criar" class="btn btn-primary">
                    </div>
            </div>



         </form>
    </div>


</div>
